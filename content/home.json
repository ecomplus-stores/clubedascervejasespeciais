y: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    text-align: center\n}\n\nsection#modulo2 .section-info .div-content {\n    display: flex;\n    align-items: center;\n    flex-direction: column;\n    justify-content: center\n}\n\nsection#modulo2 .section-info .div-content h3 {\n    font-weight: 500;\n    font-size: 2.2em;\n    padding: 2.2rem 0 1rem;\n    font-family: neo-sans, sans-serif\n}\n\nsection#modulo2 .section-info .div-content p {\n    font-weight: 400;\n    font-size: 1.4rem;\n    line-height: 21px;\n    width: 20em;\n    height: 10rem;\n    color: #474952\n}\n\nsection#modulo2 .section-escolhe .div-content h3,\nsection#modulo2 .section-comodidade .div-content h3 {\n    color: #3c214b\n}\n\nsection#modulo2 .section-curadoria .div-content h3 {\n    color: #00826b\n}\n\nsection#modulo2 .section-curadoria .div-img img {\n    height: 227px;\n    object-fit: contain\n}\n\nsection#modulo2 .div-button {\n    display: flex;\n    justify-content: center\n}\n\n@media(max-width: 768px) {\n    section#modulo2 h2 {\n        width: 30rem\n    }\n\n    section#modulo2 p.tx1 {\n        margin-bottom: 4rem;\n        width: 32rem\n    }\n}\n\n@media(min-width: 1000px) {\n    section#modulo2 .section-main-info .slick-list .slick-track {\n        display: flex;\n        justify-content: center;\n        width: 93rem !important\n    }\n\n    section#modulo2 .section-main-info .slick-list .slick-track .slick-slide {\n        width: 244px !important\n    }\n}\n\n@media(min-width: 1000px) {\n    section#modulo2 {\n        padding: 1.25rem\n    }\n\n    section#modulo2 h2 {\n        font-size: 36px\n    }\n\n    section#modulo2 h2 span.txGrow {\n        font-size: 1em\n    }\n\n    section#modulo2 .section-main-info {\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        padding: 2rem 0 1rem\n    }\n\n    section#modulo2 .section-main-info .section-info:not(.section-info:first-child) {\n  